<BowlScreen>:
    BoxLayout:
        orientation: 'vertical'
        padding: dp(20)
        spacing: dp(15)

        BoxLayout:
            orientation: 'horizontal'
            size_hint_y: 0.1
            spacing: dp(10)

            MDIconButton:
                icon: "chevron-left"
                theme_text_color: "Custom"
                text_color: 1, 1, 1, 1
                pos_hint: {"center_x": 0, "center_y": 0.5}  # Left side
                halign: 'left'

            Widget:  # Spacer to push buttons apart

            MDIconButton:
                icon: "chevron-right"
                theme_text_color: "Custom"
                text_color: 1, 1, 1, 1
                pos_hint: {"center_x": 1, "center_y": 0.5}  # Right side
                halign: 'right'

        Label:
            text: "Bowl Screen"
            font_size: "28sp"
            bold: True
            halign: "center"
            size_hint_y: None
            height: dp(40)
            color: 1, 1, 1, 1

        # Removed the extra white box here
        

        GridLayout:
            cols: 3
            spacing: dp(10)
            padding: dp(10)
            size_hint_y: 0.2

            MDRaisedButton:
                id: start_button
                text: "Start"
                font_size: "18sp"
                size_hint_y: None
                height: dp(50)
                md_bg_color: (0.5, 0.5, 0.5, 1)  # Grey color
                theme_text_color: "Custom"
                text_color: 1, 1, 1, 1

            MDRaisedButton:
                id: stop_button
                text: "Stop"
                font_size: "18sp"
                size_hint_y: None
                height: dp(50)
                md_bg_color: (0.5, 0.5, 0.5, 1)  # Grey color
                theme_text_color: "Custom"
                text_color: 1, 1, 1, 1

            MDRaisedButton:
                id: end_button
                text: "End"
                font_size: "18sp"
                size_hint_y: None
                height: dp(50)
                md_bg_color: (0.5, 0.5, 0.5, 1)  # Grey color
                theme_text_color: "Custom"
                text_color: 1, 1, 1, 1
            
            MDRaisedButton:
                id: reset_button
                text: "Reset"
                font_size: "18sp"
                size_hint_y: None
                height: dp(50)
                md_bg_color: (0.5, 0.5, 0.5, 1)  # Grey color
                theme_text_color: "Custom"
                text_color: 1, 1, 1, 1

            MDRaisedButton:
                id: save_button
                text: "Save"
                font_size: "18sp"
                size_hint_y: None
                height: dp(50)
                md_bg_color: (0.5, 0.5, 0.5, 1)  # Grey color
                theme_text_color: "Custom"
                text_color: 1, 1, 1, 1

