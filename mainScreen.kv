<UserScreen>
    name: 'user_screen'
    BoxLayout:
        orientation: 'vertical'
        
        Label:
            text:'User Screen'
            align: 'center'
    
<MainScreen>
    # Set screen name
    name: 'MainScreen'
    

    BoxLayout:
        orientation: 'vertical'
 
        BoxLayout:
            orientation: 'horizontal'
            padding: [20,5]
            size_hint_y: 0.07

            MDLabel:
                text:'Hi, Theus!'
                bold: True
                font_style:'H5'
                halign: 'left'
                pos_hint: {'left': 10}
                size_hint_x: 3

            Image:
                source: 'assets/APTLogoWhite.png'
                keep_ratio: True

            
        

        MDBottomNavigation:
            id:main_screen_nav

            MDBottomNavigationItem:
                name:'home'
                text:'Home'
                icon:'home'
                HomeScreen:
                    id:homescreen
            MDBottomNavigationItem:
                name:'live'
                text:'Live'
                icon:'video-box'
                LiveScreen:
                    id:livescreen
            MDBottomNavigationItem:
                name:'video'
                text:'Videos'
                icon:'play-box-multiple'
                VideoScreen:
                    id:videoscreen
            MDBottomNavigationItem:
                name:'user'
                text:'User'
                icon:'account-circle'
                UserScreen:

<SplashScreen>:
    # Set sc screen name
    name: "SplashScreen"
    
    # Start the progress bar animation when the screen is active
    MDBoxLayout:
        #md_bg_color: 0.1, 0.1, 0.1, 1
        orientation: 'vertical'
        padding: [0,300]
            
        # Add App Image
        Image:
            source:'assets/APTLogoWhite.png'
            allow_stretch: True
            pos_hint: {'center_x': 0.5, 'center_y': 0.1}
            size_hint: (0.8, 1)

    
        # Add progress bar
        MDProgressBar:
            pos_hint: {'center_x': 0.5, 'center_y': 0.9}
            size_hint: (0.7, 0.1)
            # Set progress id
            id: progress
            # Set orientation
            orientation: "horizontal"
            # Set progress type
            type: "indeterminate"
            # Progress duration
            running_duration: 1
            # Catching duration
            catching_duration: 1

        MDLabel:
            text:'Powered By'
            font_style:'H6'
            # Horizontal text alignment
            halign: "center"
            text_color: 1, 1, 1, 1
            pos_hint: {'center_x': 0.5, 'center_y': 0.2}
            size_hint: (0.7, 0.1)


        # Add App Image
        Image:
            source:'assets/BrilliantAILogoWhite.png'
            allow_stretch: True
            size_hint: (0.4, 0.5)
            pos_hint: {'center_x': 0.5, 'center_y': 0.17}

ScreenManager:
    id: screen_manager

    SplashScreen:
        id: splashscreen

    MainScreen:
        id:mainscreen

    
